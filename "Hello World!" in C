#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
	
    char s[100];
    scanf("%[^\n]%*c", &s);
  	
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char s1[100] = "Hello, World!";
    printf("%s",s1);
    printf("\n%s",s);
    return 0;
}
